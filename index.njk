---
layout: base-layout.njk
pagination:
  data: collections.project
  size: 10
  reverse: true
  alias: projects
---
{% for project in projects %}
  <article>
    <h1>
      <a href="{{ project.url | url }}">{{ project.data.title }}</a>
    </h1>

    {% excerpt project %}

    <a href="{{ project.url | url }}">Read more</a>
  </article>
{% endfor %}
